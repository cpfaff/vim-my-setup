snippet     bun
abbr        NeoBundle 'id'
prev_word   '^'
   NeoBundle '${1:bundle_name}'

snippet header
   " File: ${1:`expand('%:t')`}
   " Author: ${2:`g:snips_author`}
	" Description: ${3}
	${4:" Last Modified: `strftime("%B %d, %Y")`}

snippet ifex
abbr if_exists
	if exists(${1:whatever})
		finish
	endif
	let $1 = ${3}

